                f_selfOrClientSyncAndVerdict(c_poDone, f_getVerdict()); 
                log("*** " & __SCOPE__ & ": INFO: Postamble done. ***");
                
            } // End of function f_TP_S1AP_MME_RAB_01                
            
        } // End of group ERAB_management_group
        
    } // End of group MME_Role
    
